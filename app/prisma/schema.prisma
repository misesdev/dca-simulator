generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model prices {
    id Int @id @default(autoincrement())
    code String @db.VarChar(10)
    codein String @db.VarChar(10)
    high String @db.VarChar(15)
    low String @db.VarChar(15)
    timestamp BigInt
    day Int
    month Int
    year Int
    @@unique([day, month, year, codein])
    @@index([year])
    @@index([month])
    @@index([day])
}


